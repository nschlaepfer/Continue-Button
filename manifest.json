{
    "manifest_version": 3,
    "name": "Custom Button Extension",
    "version": "1.0",
    "description": "Adds a custom button with event listener to the page.",
    "permissions": [
      "activeTab"
    ],
    "action": {
      "default_popup": "",
      "default_icon": {
        "48": "icon.png"
      }
    },
    "icons": {
      "48": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["https://chat.openai.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  